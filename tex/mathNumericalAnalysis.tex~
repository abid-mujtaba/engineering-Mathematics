\hline
\Tstrut 
-3&-27&&&&\\
&&19&&&\\
-2&-8&&-12&&\\
&&7&&6&\\
-1&-1&&-6&&0\\
&&1&&6&\\
0&0&&0&&0\\
&&1&&6&\\
1&1&&6&&0\\
&&7&&6&\\
