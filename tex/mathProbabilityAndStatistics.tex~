\foreach \y in {0.2,0.4,0.6,0.8}{\draw(0,0+\y)--++(0.1,0)  (0,1+\y)--++(0.1,0) (0,2+\y)--++(0.1,0) (0,3+\y)--++(0.1,0);}
\foreach \x/\xs in {1/300,2/350,3/400,4/450}{\draw(\x,0)node[below]{$\xs$}--++(0,0.2);}
