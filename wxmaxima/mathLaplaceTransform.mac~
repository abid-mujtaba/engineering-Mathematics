eq:85=(A*s+B)*(s^2+2*s+10)+(C*s+D)*(s^2+1);
expand(eq);

eq1:A+C=0;
eq2:2*A+B+D=0;
eq3:10*A+2*B+C=0;
eq4:10*B+D=85;

solve([eq1,eq2,eq3,eq4],[A,B,C,D]);


