Print("======================================");
kill(all);
print("example 3.3");

ya:0;
yb:1;
yc:-1;

eq:(y-ya)*(y-yb)*(y-yc)=0;
expand(%);

Print("======================================");
kill(all);
print("example 3.3");

m*(m-1)*(m-2)-5*m*(m-1)+12*m-12=0;
expand(%);

solve(%);

Print("======================================");
kill(all);
print("example 3.4");

eq1:c1+c2+c3=-1;
eq2:c1+3*c2+4*c3=-1;
eq3:6*c2+12*c3=0;

solve([eq1,eq2,eq3],[c1,c2,c3]);
