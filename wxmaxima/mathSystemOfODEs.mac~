kill(all);
print("Q 4.6");

A:matrix([0.02,-0.01],[0.01,-0.02]);
soln:eigenvectors(A);
solnEValue:soln[1];
solnEVector:soln[2];

EValue:solnEValue[1];

EValueA:EValue[1];
EValueB:EValue[2];
solEVectorAA:solnEVector[1];
solEVectorBB:solnEVector[2];

Print("======================================");
kill(all);
print("Q 4.6");

A:matrix([-0.01,0.02],[0.01,-0.02]);
soln:eigenvectors(A);
solnEValue:soln[1];
solnEVector:soln[2];

EValue:solnEValue[1];
EValueA:EValue[1];
EValueB:EValue[2];

solEVectorAA:solnEVector[1];
solEVectorBB:solnEVector[2];

eq1:c1+c2=0;
eq2:-c1+0.5*c2=30;
solve([eq1,eq2],[c1,c2]);

Print("======================================");
kill(all);
print("Q 4.7");

A:matrix([-0.04,0.02,0.02],[0.02,-0.02,0],[0.02,0,-0.02]);
soln:eigenvectors(A);
solnEValue:soln[1];
solnEVector:soln[2];

EValue:solnEValue[1];
EValueA:EValue[1];
EValueB:EValue[2];
EValueC:EValue[3];

solEVectorAA:solnEVector[1];
solEVectorBB:solnEVector[2];
solEVectorCC:solnEVector[3];

eq1:c1+c3=0;
eq2:-0.5*c1+c2+c3=30;
eq3:-0.5*c1-c2+c3=0;
solve([eq1,eq2,eq3],[c1,c2,c3]);

Print("======================================");
kill(all);
print("Q 4.8");

