atvalue(iC(t),t=0,2);
atvalue(i1(t),t=0,0);
atvalue(vC(t),t=0,-22);


desolve([eq1,eq2,eq3,eq4],[i1(t),iC(t),vC(t),vL(t)]);
exponentialize(%);
