print("Q 4.10");

L:2;
C:0.4;
R1:6;
R2:5;
E:12;

eq1:'diff(i1(t),t)=-3*i1(t)+3*i2(t)+6;
eq2:'diff(i2(t),t)=1/11*(-18*i1(t)+15.5*i2(t)+36);

atvalue(i1(t),t=0,0);
atvalue(i2(t),t=0,0);
desolve([eq1,eq2],[i1(t),i2(t)]);
float(%);

Print("======================================");
kill(all);
print("Q 4.11");

m1:-2;
m2:-3;

eq:(m-m1)*(m-m2);
expand(%);

a0:6;
a1:5;


eq:'diff(y,t,2)+a1*'diff(y,t,1)+a0*y=0;
ode2(eq,y,t);

A:matrix([0,1],[-a0,-a1]);
eigenvectors(A);



Print("======================================");
kill(all);
