Print("======================================");
kill(all);
print("example ");

A:matrix([2,1],[-3,0]);
B:matrix([7,1],[2,-4]);

A . B;
B . A;

Print("======================================");
kill(all);
print("example 4.1");

A:matrix([-3,3],[-0.8,0.4]);
ww:matrix([w,0],[0,w]);

M:A-ww;
determinant(M);
dt:expand(%);

eq:dt=0;
solve(%,w);
float(%);

Print("======================================");
kill(all);
print("example 4.3");

L:1;
C:0.4;
R1:6;
R2:5;
E:12;

eq1:'diff(i1(t),t)=-6*i1(t)+6*i2(t)+12;
eq2:'diff(i2(t),t)=-36/11*i1(t)+67/22*i2(t)+72/11;

atvalue(i1(t),t=0,0);
atvalue(i2(t),t=0,0);
desolve([eq1,eq2],[i1(t),i2(t)]);
exponentialize(%);
expand(%);
float(%);


Print("======================================");
kill(all);
load("vect");
print("example 4.3 using KCL, KVL");

L:1;
C:0.4;
R1:6;
R2:5;
E:12;

eq1:iC(t)=C*'diff(vC(t),t);
eq2:vL(t)=L*'diff(i1(t),t);
eq3:E=vL(t)+(i1(t)-iC(t))*R1;
eq4:vC(t)+iC(t)*R2+(iC(t)-i1(t))*R1=0;

atvalue(iC(t),t=0,0);
atvalue(i1(t),t=0,0);
atvalue(vC(t),t=0,0);


desolve([eq1,eq2,eq3,eq4],[i1(t),iC(t),vC(t),vL(t)]);
exponentialize(%);
expand(%);
float(%);

Print("======================================");
kill(all);
print("Q 4.1");

A:matrix([h/2,0],[0,-h/2]);
eigenvectors(A);

Print("======================================");
kill(all);
