print("example 9.8");

L:matrix([0,2.3,0.4],[0.6,0,0],[0,0.3,0]);
xA:matrix([500,500,500]);
xA:matrix([2600,2600,2600]);
I:matrix([1,0,0],[0,1,0],[0,0,1]);

xB:L . xA;
xC: L . xB;
xD: L . xC;

LL:L-k*I;
eq:determinant(LL);
solve(eq,k);
eigenvectors(L);


